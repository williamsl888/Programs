  <body>
    <h2>Javascript String concat methods</h2>
    <p id="demo"></p>
    <button onclick="concatenate()" class="btn">Click me</button>
    <script>
      function concatenate()
      {
      var text1=prompt("Please enter the first word");
      var text2=prompt("Please enter the second word");
      var text3=text1.concat(" ",text2);
      document.getElementById("demo").innerHTML=text3;
      }
      </script>
