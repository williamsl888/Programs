<script>
  var str=prompt("Enter a string");
  var len=str.length;
  var text="";
  var i;
  for(i=0;i<len;i++)
  {
  text+="str["+i+"] ="+str[i]+"<br>";
  }
document.getElementById("demo").innerHTML=text;

</script>

