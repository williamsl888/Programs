<!DOCTYPE html>
<html>
  <head>
    <script>
      var a=parseInt(prompt("Please enter a number"));
      var b=parseInt(prompt("Please enter the second number"));
      var c=parseInt(prompt("Please enter the third number"));
      if(a>b && a>c)
      {
        alert("a is largest");
      }
      else if(b>a && b>c)
      {
        alert("b is largest");
      }
      else{
        alert("c is the largest");
      }
      </script>
  </head>
</html>
